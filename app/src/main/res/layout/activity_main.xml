<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:tools="http://schemas.android.com/tools"
                android:id="@+id/mainpage"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
    android:background="@drawable/main_bg1"
                android:paddingBottom="@dimen/activity_vertical_margin"
                android:paddingLeft="@dimen/activity_horizontal_margin"
                android:paddingRight="@dimen/activity_horizontal_margin"
                android:paddingTop="@dimen/activity_vertical_margin"
                tools:context="university.pace.mypace2.MainActivity">


    <ImageButton
        android:id="@+id/campusmap"
        android:onClick="onClick"
        android:layout_height="90dp"
        android:layout_width="90dp"
        android:background="@drawable/map_icon_words"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:id="@+id/calBut"
        android:layout_height="90dp"
        android:layout_width="90dp"
        android:onClick="onClick"
        android:background="@drawable/calender_words"
        android:layout_alignTop="@+id/sss"
        android:layout_alignParentEnd="true" />

    <ImageButton
        android:id="@+id/courses"
        android:onClick="onClick"
        android:layout_height="90dp"
        android:layout_width="90dp"
        android:background="@drawable/coursesbutton2"
        android:layout_alignParentTop="true"
        android:layout_centerVertical="true"
        android:layout_alignStart="@+id/sss" />


    <ImageButton
        android:id="@+id/tutoringbutt"
        android:background="@drawable/tutoringbutton_words2"
        android:layout_height="90dp"
        android:layout_width="90dp"
        android:onClick="onClick"
        android:layout_below="@+id/campusmap"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:id="@+id/sss"
        android:onClick="onClick"
        android:layout_height="90dp"
        android:layout_width="90dp"
        android:background="@drawable/sssbutton"
        android:layout_below="@+id/courses"
        android:layout_alignStart="@+id/mentor" />

    <ImageButton
        android:id="@+id/numbers"
        android:onClick="onClick"
        android:layout_height="90dp"
        android:layout_width="90dp"
        android:background="@drawable/hotline"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true" />


    <ImageButton
        android:id="@+id/blackboard"
        android:clickable="true"
        android:layout_height="90dp"
        android:layout_width="90dp"
        android:background="@drawable/bb_words"
        android:layout_below="@+id/tutoringbutt"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:id="@+id/mentor"
        android:background="@drawable/pacementor_space"
        android:layout_height="90dp"
        android:layout_width="90dp"
        android:onClick="onClick"
        android:layout_alignTop="@+id/blackboard"
        android:layout_centerHorizontal="true" />


    <ImageButton
        android:id="@+id/pacemail"
        android:onClick="onClick"
        android:layout_height="90dp"
        android:layout_width="90dp"
        android:background="@drawable/pacemail_words"
        android:layout_alignTop="@+id/mentor"
        android:layout_alignStart="@+id/calBut" />

</RelativeLayout>
