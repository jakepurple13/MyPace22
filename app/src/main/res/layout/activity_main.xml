<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/main_bg1"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="university.pace.mypace2.MainActivity">

    <TableLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true">


        <TableRow>

            <ImageButton
                android:id="@+id/campusmap"
                android:layout_width="90dp"
                android:layout_height="90dp"


                android:onClick="onClick"
                android:clickable="true"
                android:background="@drawable/map_icon_words"
                android:layout_column="4" />

            <Space
                android:layout_width="20px"
                android:layout_height="20px"
                android:layout_column="6" />

            <Space
                android:layout_width="20px"
                android:layout_height="20px"
                android:layout_column="7" />

            <Space
                android:layout_width="20px"
                android:layout_height="20px"
                android:layout_column="8" />

            <ImageButton
                android:id="@+id/calBut"
                android:layout_width="90dp"
                android:layout_height="90dp"


                android:onClick="onClick"
                android:background="@drawable/calendar_words"
                android:layout_column="9" />

            <Space
                android:layout_width="20px"
                android:layout_height="20px"
                android:layout_column="12" />

            <Space
                android:layout_width="20px"
                android:layout_height="20px"
                android:layout_column="13" />

            <ImageButton
                android:id="@+id/courses"
                android:layout_width="90dp"
                android:layout_height="90dp"


                android:onClick="onClick"
                android:background="@drawable/coursesbutton"
                android:layout_column="16" />


        </TableRow>


        <TableRow android:layout_marginTop="16dp">


            <ImageButton
                android:id="@+id/tutoringbutt"
                android:layout_width="90dp"
                android:layout_height="90dp"
                android:background="@drawable/tutoringbutton_words2"
                android:onClick="onClick"
                android:layout_column="4" />

            <Space
                android:layout_width="20px"
                android:layout_height="20px"
                android:layout_column="6" />

            <Space
                android:layout_width="20px"
                android:layout_height="20px"
                android:layout_column="7" />

            <Space
                android:layout_width="20px"
                android:layout_height="20px"
                android:layout_column="8" />

            <ImageButton
                android:id="@+id/sss"
                android:layout_width="90dp"
                android:layout_height="90dp"

                android:onClick="onClick"
                android:background="@drawable/sssbutton_new"
                android:layout_column="9" />

            <Space
                android:layout_width="20px"
                android:layout_height="20px"
                android:layout_column="13" />

            <Space
                android:layout_width="20px"
                android:layout_height="20px" />

            <ImageButton
                android:id="@+id/numbers"
                android:layout_width="6dp"
                android:layout_height="90dp"
                android:onClick="onClick"
                android:background="@drawable/hotline_v21"
                android:layout_column="16" />


            <!---   Any buttons Put here Keep my rows Like this Jacob -->
        </TableRow>


        <TableRow android:layout_marginTop="16dp">


            <ImageButton
                android:id="@+id/blackboard"
                android:layout_width="90dp"
                android:layout_height="90dp"

                android:clickable="true"
                android:background="@drawable/bb_words"
                android:layout_column="4"
                android:onClick="onClick" />

            <Space
                android:layout_width="20px"
                android:layout_height="20px" />

            <Space
                android:layout_width="20px"
                android:layout_height="20px" />

            <Space
                android:layout_width="20px"
                android:layout_height="20px" />

            <Space
                android:layout_width="20px"
                android:layout_height="20px"
                android:layout_column="8" />

            <ImageButton
                android:layout_width="90dp"
                android:layout_height="90dp"
                android:background="@drawable/pacementor_nopace"
                android:onClick="onClick"
                android:layout_column="9" />


            <Space
                android:layout_width="20px"
                android:layout_height="20px"
                android:layout_column="10" />

            <Space
                android:layout_width="20px"
                android:layout_height="20px"
                android:layout_column="11" />

            <ImageButton
                android:id="@+id/pacemail"
                android:layout_width="90dp"
                android:layout_height="90dp"
                android:onClick="onClick"
                android:background="@drawable/pacemail_words"
                android:layout_column="16" />


            <!---   text font Copper gothic bold size 40 -->
        </TableRow>

    </TableLayout>


</RelativeLayout>
