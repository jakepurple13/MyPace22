<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:background="@color/Pace_blue_light_2"
    android:id="@+id/supportpace">


    <LinearLayout
        android:orientation="vertical"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:layout_marginRight="20dp"
        android:layout_marginLeft="20dp">


        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/Academic"
            android:id="@+id/academicbutt"
            android:textColor="#FFFFFF"
            android:background="@color/PaceBlue"
            android:textSize="37sp"
            android:layout_marginBottom="20dp"
            android:clickable="true"
            android:textStyle="bold|italic"
            android:onClick="onClick" />

        <Button
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="@string/Culture"
            android:id="@+id/culturalbutt"
            android:textColor="#FFFFFFFF"
            android:background="@color/PaceBlue"
            android:textSize="37dp"
            android:layout_marginBottom="20dp"
            android:clickable="true"
            android:textStyle="bold|italic"
            android:onClick="onClick" />

        <Button
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="@string/FinancialAid"
            android:id="@+id/Finanbutt"
            android:textSize="37dp"
            android:textColor="#FFFFFFFF"
            android:background="@color/PaceBlue"
            android:elegantTextHeight="false"
            android:clickable="true"
            android:textStyle="bold|italic"
            android:layout_marginBottom="20dp"
            android:onClick="onClick" />


        <TableLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">


                <Space
                    android:layout_width="20px"
                    android:layout_height="20px"
                    android:layout_column="7" />

                <Space
                    android:layout_width="20px"
                    android:layout_height="20px"
                    android:layout_column="8" />

                <Space
                    android:layout_width="20px"
                    android:layout_height="20px"
                    android:layout_column="9" />


                <Space
                    android:layout_width="20px"
                    android:layout_height="20px"
                    android:layout_column="10" />

                <ImageButton
                    android:id="@+id/john_email"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|right"
                    android:layout_marginBottom="16dp"
                    android:background="@drawable/pace_bubble"
                    android:elevation="8dp"
                    android:src="@drawable/hookerhoto_words"
                    android:onClick="onClick"
                    android:layout_column="11" />

                <Space
                    android:layout_width="20px"
                    android:layout_height="20px"
                    android:layout_column="12" />

                <Space
                    android:layout_width="20px"
                    android:layout_height="20px"
                    android:layout_column="13" />

                <Space
                    android:layout_width="20px"
                    android:layout_height="20px"
                    android:layout_column="14" />

                <Space
                    android:layout_width="20px"
                    android:layout_height="20px"
                    android:layout_column="15" />

                <Space
                    android:layout_width="20px"
                    android:layout_height="20px"
                    android:layout_column="16" />

                <Space
                    android:layout_width="20px"
                    android:layout_height="20px" />

                <Space
                    android:layout_width="20px"
                    android:layout_height="20px"
                    android:layout_column="17" />


                <Space
                    android:layout_width="20px"
                    android:layout_height="20px" />

                <ImageButton
                    android:id="@+id/joyce_email"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|right"
                    android:layout_marginBottom="16dp"
                    android:background="@drawable/pace_bubble"
                    android:elevation="8dp"
                    android:src="@drawable/joycephoto_words"
                    android:onClick="onClick"
                    android:layout_column="19" />


            </TableRow>

            <TableRow
                android:layout_marginLeft="10dp">


            </TableRow>

            <TableRow

                android:background="@drawable/supportteam">

            </TableRow>


        </TableLayout>


    </LinearLayout>
    <!-- set to transparent color -->


</RelativeLayout>
