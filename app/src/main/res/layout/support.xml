<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:background="@color/Pace_blue_light_2"
    android:id="@+id/supportpace">


    <LinearLayout
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:id="@+id/linearLayout">


        <Button
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="@string/Academic"
            android:id="@+id/academicbutt"
            android:textColor="#FFFFFF"
            android:background="@color/PaceBlue"
            android:textSize="37dp"
            android:layout_marginBottom="20dp"
            android:clickable="true"
            android:textStyle="bold|italic"
            android:onClick="onClick" />

        <Button
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="@string/Culture"
            android:id="@+id/SettingsLanguage"
            android:textColor="#FFFFFFFF"
            android:background="@color/PaceBlue"
            android:textSize="37dp"
            android:layout_marginBottom="20dp"
            android:clickable="true"
            android:textStyle="bold|italic" />

        <Button
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="@string/FinancialAid"
            android:id="@+id/k"
            android:textSize="37dp"
            android:textColor="#FFFFFFFF"
            android:background="@color/PaceBlue"
            android:elegantTextHeight="false"
            android:clickable="true"
            android:textStyle="bold|italic"
            android:layout_marginBottom="20dp" />


        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/emailtableLayout">

            <TableRow>

                <android.support.design.widget.FloatingActionButton
                    android:id="@+id/john_email"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/hookerhoto"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="15dp"
                    app:fabSize="normal"
                    app:borderWidth="0dp"
                    app:elevation="6dp"
                    app:pressedTranslationZ="12dp"
                    app:rippleColor="@android:color/transparent"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentEnd="true"
                    android:onClick="onClick" />

                <android.support.design.widget.FloatingActionButton
                    android:id="@+id/joyce_email"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/joycephoto"
                    android:layout_marginRight="15dp"
                    app:fabSize="normal"
                    app:borderWidth="0dp"
                    app:elevation="6dp"
                    app:pressedTranslationZ="12dp"
                    app:rippleColor="@android:color/transparent"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentEnd="true"
                    android:onClick="onClick" />

                <android.support.design.widget.FloatingActionButton
                    android:id="@+id/maria_email"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/mariaphoto"
                    android:layout_marginRight="15dp"
                    app:fabSize="normal"
                    app:borderWidth="0dp"
                    app:elevation="6dp"
                    app:pressedTranslationZ="12dp"
                    app:rippleColor="@android:color/transparent"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentStart="true"
                    android:onClick="onClick" />

                <android.support.design.widget.FloatingActionButton
                    android:id="@+id/norma_email"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/normaphoto"
                    app:fabSize="normal"
                    app:borderWidth="0dp"
                    app:elevation="6dp"
                    app:pressedTranslationZ="12dp"
                    app:rippleColor="@android:color/transparent"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentStart="true"
                    android:onClick="onClick" />
            </TableRow>

            <TableRow android:background="@drawable/supportteam">

            </TableRow>


        </TableLayout>


    </LinearLayout>
    <!-- set to transparent color -->


</RelativeLayout>
